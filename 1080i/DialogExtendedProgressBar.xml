<?xml version="1.0" encoding="utf-8"?>
<window type="dialog">
  <defaultcontrol>32</defaultcontrol>
  <animation effect="slide" start="0,-100" end="0,0" time="100">WindowOpen</animation>
  <animation effect="slide" start="0,0" end="0,-100" delay="400" time="100">WindowClose</animation>
  <controls>
    <control type="group">
      <animation effect="slide" end="0,-100" time="200" condition="Window.IsVisible(FullscreenVideo) | Window.IsVisible(Visualisation)">conditional</animation>
      <posx>1080</posx>
      <posy>0</posy>
      <control type="image" description="Dialog BG">
        <posx>0</posx>
        <posy>-6</posy>
        <width>600</width>
        <height>108</height>
        <texture border="14">img/ProgressBG.png</texture>
      </control>
      <control type="label" id="30" description="Caption Label">
        <posx>22</posx>
        <posy>9</posy>
        <width>555</width>
        <height>27</height>
        <font>size14</font>
        <align>left</align>
        <aligny>center</aligny>
      </control>
      <control type="label" id="31" description="Current Directory Label">
        <posx>22</posx>
        <posy>36</posy>
        <width>555</width>
        <height>30</height>
        <font>size14</font>
        <align>left</align>
        <aligny>center</aligny>
      </control>
      <control type="progress" id="32" description="Progress bar">
        <posx>22</posx>
        <posy>75</posy>
        <width>555</width>
        <height>8</height>
        <texturebg>MusicOSD/ProgressBG.png</texturebg>
        <midtexture>MusicOSD/ProgressMidd.png</midtexture>
        <righttexture>MusicOSD/ProgressRight.png</righttexture>
        <lefttexture>MusicOSD/ProgressLeft.png</lefttexture>
      </control>
    </control>
  </controls>
</window>
